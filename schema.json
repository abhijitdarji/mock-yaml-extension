{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "JSON Schema for mock-yaml-server",
    "description": "Mock yaml server linter and validation tool",
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "resource": {
        "type": "string"
      },
      "headers": {
        "type": "array",
        "items": {
          "type": "object",
          "patternProperties": {
            "^[A-Za-z0-9_]*$": {
              "type": "string"
            }
          }
        }
      },
      "cookies": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "value": {
              "type": "string"
            },
            "options": {
              "type": "object",
              "properties": {
                "domain": {
                  "type": "string"
                },
                "path": {
                  "type": "string"
                },
                "secure": {
                  "type": "boolean"
                },
                "httpOnly": {
                  "type": "boolean"
                },
                "expires": {
                  "type": "string"
                },
                "maxAge": {
                  "type": "string"
                },
                "sameSite": {
                  "type": "boolean"
                }
              }
            }
          },
          "required": ["name", "value"]
        }
      },
      "seed": {
        "type": "number"
      },
      "properties": {
        "type": "object",
        "patternProperties": {
          "^[A-Za-z0-9_]*$": {
            "$ref": "#/definitions/patternSchema"
          }
        }
      }
    },
    "required": ["resource", "properties"],
    "definitions": {
      "patternSchema": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "oneOf": [
              {
                "const": "compose",
                "description": "Build string using multiple faker methods"
              },
              {
                "const": "json",
                "description": "JSON object"
              },
              {
                "const": "array",
                "description": "Array of objects"
              },
              {
                "const": "eval",
                "description": "Evaluate JavaScript block"
              },
              {
                "const": "object",
                "description": "Nested object"
              },
              {
                "$ref": "#/definitions/fakerTypes"
              }
            ]
          },
          "input": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "string"
              },
              {
                "type": "object"
              },
              {
                "type": "boolean"
              },
              {
                "type": "array",
                "items": {
                  "anyOf": [
                    {
                      "type": "string"
                    },
                    {
                      "$ref": "#/definitions/fakerProp"
                    }
                  ]
                }
              }
            ]
          },
          "options": {
            "$ref": "#/definitions/optionsType"
          },
          "items": {
            "type": "array",
            "items": {
              "anyOf": [
                {
                  "$ref": "#/definitions/propertiesSchema"
                },
                {
                  "$ref": "#/definitions/fakerProp"
                },
                {
                  "$ref": "#/definitions/patternSchema"
                }
              ]
            }
          },
          "count": {
            "type": "number"
          },
          "properties": {
            "type": "object",
            "patternProperties": {
              "^[A-Za-z0-9_]*$": {
                "$ref": "#/definitions/patternSchema"
              }
            }
          }
        },
        "required": ["type"]
      },
      "propertiesSchema": {
        "type": "object",
        "properties": {
          "properties": {
            "type": "object",
            "patternProperties": {
